<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>crear</servlet-name>
        <servlet-class>.ControladorEstudiante</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>controlador.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorInicioSesion</servlet-name>
        <servlet-class>controlador.ControladorInicioSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorEstudianteCrear</servlet-name>
        <servlet-class>controlador.ControladorEstudianteCrear</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorReservaCrear</servlet-name>
        <servlet-class>controlador.ControladorReservaCrear</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorEstudianteConsultar</servlet-name>
        <servlet-class>controlador.ControladorEstudianteConsultar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorReservaConsultar</servlet-name>
        <servlet-class>controlador.ControladorReservaConsultar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorReservaCancelar</servlet-name>
        <servlet-class>controlador.ControladorReservaCancelar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControladorEstudianteConsultar2</servlet-name>
        <servlet-class>controlador.ControladorEstudianteConsultar2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Faces Servlet</servlet-name>
        <url-pattern>/faces/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crear</servlet-name>
        <url-pattern>/crear</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorInicioSesion</servlet-name>
        <url-pattern>/ControladorInicioSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorEstudianteCrear</servlet-name>
        <url-pattern>/ControladorEstudianteCrear</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorReservaCrear</servlet-name>
        <url-pattern>/ControladorReservaCrear</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorEstudianteConsultar</servlet-name>
        <url-pattern>/ControladorEstudianteConsultar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorReservaConsultar</servlet-name>
        <url-pattern>/ControladorReservaConsultar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorReservaCancelar</servlet-name>
        <url-pattern>/ControladorReservaCancelar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControladorEstudianteConsultar2</servlet-name>
        <url-pattern>/ControladorEstudianteConsultar2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>faces/index.xhtml</welcome-file>
    </welcome-file-list>
</web-app>
